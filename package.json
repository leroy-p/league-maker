{
  "name": "league-maker",
  "description": "League Maker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:graphql": "graphql-codegen --config graphql-codegen.yml",
    "build:cpx": "cpx 'src/**/*.graphql' lib",
    "lingui:add": "lingui add-locale",
    "lingui:extract": "yarn run lingui extract",
    "build:lingui": "lingui compile && shx rm -f src/locales/**/*.d.ts"
  },
  "dependencies": {
    "@apollo/client": "3.5.8",
    "@lingui/react": "3.13.3",
    "@mui/icons-material": "5.6.2",
    "@mui/lab": "5.0.0-alpha.80",
    "@mui/material": "5.6.4",
    "@mui/styled-engine-sc": "5.6.1",
    "@mui/styles": "5.6.2",
    "apollo-upload-client": "17.0.0",
    "graphql-request": "4.2.0",
    "react": "18.1.0",
    "react-apollo-network-status": "5.1.0",
    "react-dom": "18.1.0",
    "react-helmet": "6.1.0",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.5",
    "typescript": "4.4.2"
  },
  "devDependencies": {
    "@lingui/cli": "3.13.0",
    "@graphql-tools/merge": "8.2.6",
    "@graphql-tools/schema": "8.3.6",
    "@graphql-tools/utils": "8.6.5",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@types/apollo-upload-client": "17.0.0",
    "@types/node": "16.7.13",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@types/react-helmet": "6.1.5",
    "@types/styled-components": "5.1.25",
    "@types/zen-observable": "0.8.3",
    "cpx": "1.5.0",
    "shx": "0.3.4",
    "graphql": "16.5.0",
    "type-graphql": "1.1.1"
  },
  "resolutions":{
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
